                </div>

                @switch (Model.LoginAction)
                {
                    case LoginAction.ForgotPassword:
                        @Html.Partial("_LoginForgotForm", Model as LoginForgotPasswordModel)
                        break;

                    case LoginAction.ResetPassword:
                        @Html.Partial("_LoginResetForm", Model as LoginResetPasswordModel)
                        break;

                    default:
                        @Html.Partial("_LoginForm", Model)
                        break;
                }
